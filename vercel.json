{
    "version": 2,
    "builds": [
        {
            "src": "server.js",  // or whatever your main file is named
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"  // or whatever your main file is named
        }
    ]
} 